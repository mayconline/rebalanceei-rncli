{
  "name": "rebalanceei",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "npx react-native start",
    "test": "jest --coverage=false --maxWorkers=50%",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "dev": "npx react-native run-android",
    "prod": "npx react-native run-android --variant=release",
    "prepare": "husky install"
  },
  "dependencies": {
    "@amplitude/react-native": "2.16.2",
    "@apollo/client": "3.7.1",
    "@react-native-async-storage/async-storage": "1.17.11",
    "@react-native-community/netinfo": "9.3.7",
    "@react-native-community/progress-bar-android": "1.0.5",
    "@react-native-masked-view/masked-view": "0.2.8",
    "@react-navigation/bottom-tabs": "6.4.3",
    "@react-navigation/native": "6.0.16",
    "@react-navigation/stack": "6.3.7",
    "axios": "1.2.1",
    "graphql": "16.6.0",
    "lottie-react-native": "5.1.4",
    "react": "18.2.0",
    "react-native": "0.69.5",
    "react-native-gesture-handler": "2.8.0",
    "react-native-google-mobile-ads": "9.0.0",
    "react-native-iap": "12.4.14",
    "react-native-linear-gradient": "2.6.2",
    "react-native-reanimated": "2.13.0",
    "react-native-safe-area-context": "4.4.1",
    "react-native-screens": "3.18.2",
    "react-native-splash-screen": "3.3.0",
    "react-native-svg": "13.7.0",
    "react-native-svg-charts": "5.4.0",
    "react-native-vector-icons": "9.2.0",
    "styled-components": "5.3.6",
    "use-debounce": "9.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.18.6",
    "@babel/plugin-transform-named-capturing-groups-regex": "7.18.6",
    "@babel/runtime": "7.18.6",
    "@react-native-community/eslint-config": "3.2.0",
    "@testing-library/jest-native": "5.4.1",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/react-native": "9.1.0",
    "@types/jest": "27.5.2",
    "@types/react-native": "0.71.0",
    "@types/react-native-svg-charts": "5.0.12",
    "@types/react-native-vector-icons": "6.4.13",
    "@types/react-test-renderer": "18.0.0",
    "@types/styled-components": "5.1.26",
    "@types/styled-components-react-native": "5.2.1",
    "@typescript-eslint/eslint-plugin": "5.48.1",
    "@typescript-eslint/parser": "5.48.1",
    "axios-mock-adapter": "1.21.2",
    "babel-jest": "27.5.1",
    "eslint": "8.31.0",
    "husky": "8.0.3",
    "jest": "27.5.1",
    "jest-styled-components": "7.1.1",
    "metro-react-native-babel-preset": "0.74.0",
    "react-test-renderer": "18.2.0",
    "typescript": "4.9.4"
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "setupFiles": [
      "./src/setupTests.ts",
      "./node_modules/react-native-gesture-handler/jestSetup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|react-navigation)/"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/pages/**/*.tsx",
      "src/pages/**/**/*.tsx",
      "src/components/**/*.tsx",
      "src/modals/**/*.tsx",
      "src/contexts/*.tsx"
    ]
  }
