{
  "name": "rebalanceei",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "npx react-native start",
    "test": "jest --coverage=false --maxWorkers=50%",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "dev": "npx react-native run-android",
    "prod": "npx react-native run-android --mode=release",
    "prepare": "husky install"
  },
  "dependencies": {
    "@amplitude/react-native": "2.17.3",
    "@apollo/client": "3.13.5",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/netinfo": "11.4.1",
    "@react-native-masked-view/masked-view": "0.3.2",
    "@react-navigation/bottom-tabs": "7.9.0",
    "@react-navigation/native": "7.1.26",
    "@react-navigation/stack": "7.6.13",
    "axios": "1.13.2",
    "graphql": "16.10.0",
    "lottie-react-native": "7.3.4",
    "react": "19.2.3",
    "react-native": "0.83.1",
    "react-native-gesture-handler": "2.30.0",
    "react-native-google-mobile-ads": "16.0.1",
    "react-native-iap": "14.7.1",
    "react-native-linear-gradient": "2.8.3",
    "react-native-nitro-modules": "^0.32.1",
    "react-native-progress": "^5.0.1",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "4.19.0",
    "react-native-splash-screen": "3.3.0",
    "react-native-svg": "15.15.1",
    "react-native-svg-charts": "5.4.0",
    "react-native-vector-icons": "10.3.0",
    "styled-components": "6.2.0",
    "use-debounce": "10.0.6",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@babel/core": "7.25.2",
    "@babel/plugin-transform-named-capturing-groups-regex": "7.18.6",
    "@babel/preset-env": "7.25.3",
    "@babel/runtime": "7.25.0",
    "@biomejs/biome": "^1.9.4",
    "@react-native-community/cli": "20.1.0",
    "@react-native-community/cli-platform-android": "20.1.0",
    "@react-native-community/cli-platform-ios": "20.1.0",
    "@react-native/babel-preset": "0.83.1",
    "@react-native/eslint-config": "0.83.1",
    "@react-native/metro-config": "0.83.1",
    "@react-native/typescript-config": "0.83.1",
    "@testing-library/jest-native": "5.4.3",
    "@testing-library/react-native": "13.2.0",
    "@types/jest": "29.5.14",
    "@types/react": "19.2.7",
    "@types/react-native-svg-charts": "5.0.12",
    "@types/react-native-vector-icons": "6.4.13",
    "@types/react-test-renderer": "19.1.0",
    "@types/styled-components": "5.1.34",
    "@types/styled-components-react-native": "5.2.5",
    "@typescript-eslint/eslint-plugin": "5.55.0",
    "@typescript-eslint/parser": "5.55.0",
    "axios-mock-adapter": "2.1.0",
    "eslint": "8.36.0",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "jest-styled-components": "7.2.0",
    "react-native-dotenv": "^3.4.11",
    "react-test-renderer": "19.2.3",
    "typescript": "5.9.3"
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "setupFiles": [
      "./src/setupTests.ts",
      "./node_modules/react-native-gesture-handler/jestSetup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(@react-native|react-native|react-native-progress|react-navigation|@apollo/client|@react-native-community|@react-navigation|react-native-reanimated|react-native-vector-icons)/)"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "moduleNameMapper": {
      "^react-native-reanimated$": "react-native-reanimated/mock",
      "^react-native-vector-icons/(.*)$": "<rootDir>/node_modules/react-native-vector-icons/$1"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/pages/**/*.tsx",
      "src/pages/**/**/*.tsx",
      "src/components/**/*.tsx",
      "src/modals/**/*.tsx",
      "src/contexts/*.tsx",
      "src/contexts/**/*.tsx",
      "src/hooks/*.tsx"
    ],
    "testTimeout": 15000
  }
}
